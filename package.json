{
  "name": "staxly",
  "version": "1.0.0",
  "description": "",
  "author": "Philip Schatz <phil@cnx.org>",
  "license": "ISC",
  "repository": "https://github.com/openstax/staxly.git",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "lint": "standard --fix",
    "start": "probot run ./index.js",
    "test": "IGNORE_FOR_TESTING=true jest && standard --fix",
    "test:debug": "NODE_ENV=development IGNORE_FOR_TESTING=true node --inspect-brk $(npm bin)/jest -i --updateSnapshot",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@slack/client": "^4.0.0",
    "probot": "^9.9.1",
    "probot-addon-autolabeler": "github:mithro/autolabeler#38392de5e611fe4538eeb48f0227e8c404439a07",
    "probot-addon-settings": "github:probot/settings#af40e21548181e8221a01bbcf125d1ee28f50a48",
    "smee-client": "^1.0.1"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nock": "^11.7.2",
    "nodemon": "^1.14.11",
    "standard": "^11.0.0"
  },
  "engines": {
    "node": ">= 12.13.1",
    "npm": ">= 6.11.3"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
